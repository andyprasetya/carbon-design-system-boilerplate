{
  "name": "carbon-design-system-boilerplate",
  "version": "1.0.0",
  "description": "A Carbon Design System (IBM) and Rollup boilerplate for webapps",
  "main": "rollup.config.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyprasetya/carbon-design-system-boilerplate.git"
  },
  "keywords": [],
