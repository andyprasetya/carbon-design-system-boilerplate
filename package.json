  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andyprasetya/carbon-design-system-boilerplate/issues"
  },
  "homepage": "https://github.com/andyprasetya/carbon-design-system-boilerplate#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "latest",
    "@rollup/plugin-json": "latest",
    "@rollup/plugin-node-resolve": "latest",
    "autoprefixer": "latest",
    "node-sass": "latest",
    "npm-run-all": "latest",
    "rollup": "latest",
    "rollup-plugin-postcss": "latest",
    "rollup-plugin-terser": "latest",
    "serve": "latest"
  },
  "dependencies": {
    "crypto-js": "latest",
